            <characteristic name="Cast/Unbind" typeId="9a5b-68ee-bc46-6a9d">1/1</characteristic>
            <characteristic name="Spells Known" typeId="25bd-2ad0-e570-e880">Arcane Bolt (Prime Electrids), Mystic Shield, Storm Lance</characteristic>
