    <selectionEntry id="3167-e3f7-2405-0c83" name="Celestant-Prime, Hammer of Sigmar" publicationId="0624-d006-ae10-12c0" hidden="false" collective="false" import="true" type="unit">
      <constraints>
        <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" id="eac9-473a-15f2-af59" type="max"/>
