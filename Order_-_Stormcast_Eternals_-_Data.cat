      <profiles>
        <profile id="f2a3-74e6-85df-8b58" name="Stormsire&apos;s Cursebreakers" hidden="false" typeId="3956-f6dd-6c88-ae4b" typeName="Unit">
          <characteristics>
            <characteristic name="Move" typeId="5bbe-8910-4dfa-7b1c">5&quot;</characteristic>
            <characteristic name="Wounds" typeId="a0c6-af64-2f24-5759">3</characteristic>
            <characteristic name="Save" typeId="93b6-2f96-2df7-e330">4+</characteristic>
            <characteristic name="Bravery" typeId="2420-1c5c-1016-7033">8</characteristic>
