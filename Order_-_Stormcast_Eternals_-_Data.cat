      <profiles>
        <profile id="a846-bb80-3436-3141" name="Celestant-Prime, Hammer of Sigmar" hidden="false" typeId="3956-f6dd-6c88-ae4b" typeName="Unit">
          <characteristics>
            <characteristic name="Move" typeId="5bbe-8910-4dfa-7b1c">12&quot;</characteristic>
            <characteristic name="Wounds" typeId="a0c6-af64-2f24-5759">8</characteristic>
