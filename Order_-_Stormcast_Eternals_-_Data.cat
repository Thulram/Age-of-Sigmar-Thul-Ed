    <selectionEntry id="027e-aa0a-3883-363c" name="Averon Stormsire" publicationId="0624-d006-ae10-12c0" hidden="false" collective="false" import="true" type="unit">
      <profiles>
        <profile id="501f-a875-52dd-1ebd" name="Averon Stormsire" hidden="false" typeId="3956-f6dd-6c88-ae4b" typeName="Unit">
          <characteristics>
            <characteristic name="Move" typeId="5bbe-8910-4dfa-7b1c">5&quot;</characteristic>
