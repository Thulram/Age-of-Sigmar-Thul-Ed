  <sharedSelectionEntryGroups>
    <selectionEntryGroup id="fa83-c120-9f4a-dd7a" name="Mount Traits" hidden="false" collective="false" import="true">
      <modifiers>
        <modifier type="set" field="hidden" value="true">
