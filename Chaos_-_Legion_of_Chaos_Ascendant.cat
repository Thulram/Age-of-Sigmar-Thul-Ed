    <selectionEntry id="c8f3-d2c6-e013-1242" name="Bladebringer, Herald on Exalted Chariot" publicationId="29d6-453f-ecc0-3e4b" hidden="false" collective="false" import="true" type="unit">
      <profiles>
        <profile id="27ca-81f1-bb0d-1997" name="Bladebringer" hidden="false" typeId="3956-f6dd-6c88-ae4b" typeName="Unit">
          <characteristics>
            <characteristic name="Move" typeId="5bbe-8910-4dfa-7b1c">10&quot;</characteristic>
            <characteristic name="Wounds" typeId="a0c6-af64-2f24-5759">10</characteristic>
