        <profile id="80df-405c-5ce6-03c5" name="Burning Bright" hidden="false" typeId="7861-da01-01c0-88d9" typeName="Ability">
          <characteristics>
            <characteristic name="Ability" typeId="9bce-4deb-8b79-eaec">Ignore modifiers (positive or negative) when making save rolls for attacks that target this unit. In addition, this unit can run and still charge later in the same turn.</characteristic>
          </characteristics>
        </profile>
        <profile id="7c19-aa0c-2140-3aeb" name="Kiss of Fire" hidden="false" typeId="7861-da01-01c0-88d9" typeName="Ability">
