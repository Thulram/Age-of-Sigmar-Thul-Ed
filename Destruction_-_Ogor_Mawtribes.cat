                  <profiles>
                    <profile id="0d70-313b-5faf-904a" name="Shrapnel Powder" hidden="false" typeId="c0c2-177b-e761-ec41" typeName="Allegiance Abilities">
                      <characteristics>
                        <characteristic name="Allegiance Ability" typeId="0506-f62d-68b3-5499">Improve the Rend characteristic of missile weapons used by friendly UNDERGUTS LEADBELCHERS and UNDERGUTS IRONBLASTER units by 1.</characteristic>
                      </characteristics>
                    </profile>
                  </profiles>
                  <costs>
