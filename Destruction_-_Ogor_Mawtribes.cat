    <selectionEntry id="4be1-8c96-af66-db38" name="Terrain Feature - Mawpit" hidden="false" collective="false" import="true" type="upgrade">
      <modifiers>
        <modifier type="set" field="1864-332a-e34d-1ec9" value="0.0">
          <conditions>
            <condition field="selections" scope="force" value="0.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" childId="6fae-e799-6e73-c03c" type="greaterThan"/>
