    <selectionEntry id="92e8-d9c3-0cea-afbe" name="Extra Command Point" hidden="false" collective="false" import="true" type="upgrade">
      <constraints>
        <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" id="6e26-8d7d-f9f0-736c" type="max"/>
      </constraints>
      <profiles>
        <profile id="a090-153f-163d-13e6" name="Extra Command Point" hidden="false" typeId="ce32-0c40-1990-4c8a" typeName="Restriction">
          <characteristics>
            <characteristic name="Restriction" typeId="edbe-5fe5-1142-afa6">You begin the battle with one additional Command Point.</characteristic>
          </characteristics>
        </profile>
      </profiles>
      <categoryLinks>
        <categoryLink id="521c-fc7f-dff7-e371" name="New CategoryLink" hidden="false" targetId="ecf9-78bd-d326-2a82" primary="true"/>
