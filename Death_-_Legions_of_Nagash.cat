            <selectionEntry id="ad61-da76-01af-dd9d" name="6 -- Ruby Vial" hidden="false" collective="false" import="true" type="upgrade">
              <constraints>
                <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" id="e4f5-2379-3579-7421" type="max"/>
