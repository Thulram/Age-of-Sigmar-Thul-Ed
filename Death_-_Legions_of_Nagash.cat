            <selectionEntry id="6021-1aeb-23a1-062c" name="6 -- Amulet of Screams" hidden="false" collective="false" import="true" type="upgrade">
              <constraints>
                <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" id="ca6a-9e80-0284-a281" type="max"/>
