                <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" id="27aa-37d0-ca02-3349" type="max"/>
              </constraints>
              <profiles>
                <profile id="b361-01aa-f984-7935" name="Shard of Night" hidden="false" typeId="1504-23bf-8850-4f65" typeName="Command Traits">
                  <characteristics>
                    <characteristic name="Command Trait" typeId="0145-9acf-f5dc-d543">Subtract 1 from hit rolls of attacks that target the bearer in the shooting phase.</characteristic>
                  </characteristics>
                </profile>
              </profiles>
              <costs>
