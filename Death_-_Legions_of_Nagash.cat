            <selectionEntry id="293e-33e8-ec0d-8c84" name="4 -- Sigil of the Sanguine Throne" hidden="false" collective="false" import="true" type="upgrade">
              <constraints>
                <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" id="86ad-ea4e-c9b0-647e" type="max"/>
              </constraints>
              <profiles>
                <profile id="b144-792d-da14-2b34" name="Sigil of the Sanguine Throne" hidden="false" typeId="0c9d-7abe-a53c-6a37" typeName="Artefacts">
                  <characteristics>
                    <characteristic name="Artefact" typeId="44b6-de26-0b54-1318">Once per charge phase, you can re-roll a failed charge roll made for friendly VAMPIRE units within 12&quot; of the bearer.</characteristic>
                  </characteristics>
                </profile>
              </profiles>
              <costs>
                <cost name="pts" typeId="b0b4-6aec-82c2-02ae" value="0.0"/>
