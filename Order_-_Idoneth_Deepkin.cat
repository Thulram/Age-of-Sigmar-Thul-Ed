                <selectionEntry id="57c8-fcc4-76dd-6fa5" name="Mor&apos;phann" hidden="false" collective="false" import="true" type="upgrade">
                  <profiles>
                    <profile id="9b29-1f4f-f600-297a" name="Soul-magic Adepts" hidden="false" typeId="c0c2-177b-e761-ec41" typeName="Allegiance Abilities">
                      <characteristics>
                        <characteristic name="Allegiance Ability" typeId="0506-f62d-68b3-5499">If the Lurelight ability of a friendly MORâ€™PHANN SOULRENDER affects a friendly MORâ€™PHANN NAMARTI unit, add 3 to the number of slain models that are returned to that unit.</characteristic>
                      </characteristics>
                    </profile>
                  </profiles>
                  <categoryLinks>
