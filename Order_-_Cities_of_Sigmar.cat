            <selectionEntry id="2ab4-2897-06a5-be2a" name="1. Unparalleled Duellist" hidden="false" collective="false" import="true" type="upgrade">
              <profiles>
                <profile id="20b9-7cf9-5165-8391" name="Unparalleled Duellist" hidden="false" typeId="1504-23bf-8850-4f65" typeName="Command Traits">
                  <characteristics>
                    <characteristic name="Command Trait" typeId="0145-9acf-f5dc-d543">For each hit roll for an attack made with a melee weapon that targets this general that does not score a hit, the attacking unit suffers 1 mortal wound after all of its attacks have been resolved.</characteristic>
                  </characteristics>
                </profile>
              </profiles>
              <costs>
