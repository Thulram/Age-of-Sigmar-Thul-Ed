            <selectionEntry id="8aac-a7c4-3f30-23ea" name="2 -- Venomfang Blade" hidden="false" collective="false" import="true" type="upgrade">
              <constraints>
                <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" id="fc82-6c82-cea0-85cb" type="max"/>
