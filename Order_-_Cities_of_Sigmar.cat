                  <characteristics>
                    <characteristic name="Command Trait" typeId="0145-9acf-f5dc-d543">WIZARD only. Add 1 to casting and unbinding rolls made for this general.</characteristic>
                  </characteristics>
                </profile>
              </profiles>
              <costs>
                <cost name="pts" typeId="b0b4-6aec-82c2-02ae" value="0.0"/>
