                  <characteristics>
                    <characteristic name="Command Trait" typeId="0145-9acf-f5dc-d543">FREEGUILD only. Wound rolls made for attacks that target this general only wound on an unmodified 4+.</characteristic>
                  </characteristics>
                </profile>
              </profiles>
              <costs>
                <cost name="pts" typeId="b0b4-6aec-82c2-02ae" value="0.0"/>
