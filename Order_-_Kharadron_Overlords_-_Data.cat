        <profile id="c0d1-8067-35fb-b49f" name="The Admiral&apos;s Flagship" hidden="false" typeId="7861-da01-01c0-88d9" typeName="Ability">
          <characteristics>
            <characteristic name="Use" typeId="9bce-4deb-8b79-eaec">Once Per Battle (Army), Deployment Phase</characteristic>
            <characteristic name="Declare" typeId="f9c7-7dae-69e1-f924">Pick an Arkanaut Ironclad or Arkanaut Frigate in this unit’s regiment to be the target.</characteristic>
            <characteristic name="Effect" typeId="7d15-5e9e-83a9-d696">For the rest of the battle, each time the target uses the ‘All-out Attack’ command while this unit is wholly within 12&quot; of it, no command points are spent.</characteristic>
          </characteristics>
        </profile>
        <profile id="9795-a244-c73f-9cf4" name="Always Take What You Are Owed" hidden="false" typeId="7861-da01-01c0-88d9" typeName="Ability">
          <characteristics>
            <characteristic name="Use" typeId="9bce-4deb-8b79-eaec">Your Hero Phase</characteristic>
            <characteristic name="Declare" typeId="f9c7-7dae-69e1-f924">Pick a friendly Arkanaut Company unit wholly within 12&quot; of this unit to be the target.</characteristic>
            <characteristic name="Effect" typeId="7d15-5e9e-83a9-d696">Add D6 to the target’s control score until the start of your next turn.</characteristic>
          </characteristics>
        </profile>
        <profile id="a7e0-17ed-dc4c-ff01" name="&apos;Bring Every Gun to Bear!&apos;" hidden="false" typeId="7861-da01-01c0-88d9" typeName="Ability">
