      <profiles>
        <profile id="a49a-b8fd-a0d2-01ca" name="Brokk Grungsson, Lord Magnate of Barak-Nar" hidden="false" typeId="3956-f6dd-6c88-ae4b" typeName="Unit">
          <characteristics>
            <characteristic name="Move" typeId="5bbe-8910-4dfa-7b1c">12&quot;</characteristic>
            <characteristic name="Wounds" typeId="a0c6-af64-2f24-5759">8</characteristic>
