        <selectionEntry id="bbb4-a028-54b8-143c" name="Kharadron Overlords" publicationId="e97a-4837-a772-7406" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="64d8-febe-acca-c932" type="max"/>
