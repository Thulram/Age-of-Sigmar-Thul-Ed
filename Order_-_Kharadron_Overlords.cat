            <selectionEntry id="fbff-d05b-d609-7361" name="4 -- Spell in a Bottle" hidden="false" collective="false" import="true" type="upgrade">
              <modifiers>
                <modifier type="set" field="hidden" value="true">
                  <conditions>
                    <condition field="selections" scope="ancestor" value="0.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" childId="7011-cb52-a745-5d33" type="notInstanceOf"/>
