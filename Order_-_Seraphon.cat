                <profile id="1415-1b72-60df-528b" name="Drain Magic" hidden="false" typeId="13da-11a8-b7d1-77a7" typeName="Spell">
                  <characteristics>
                    <characteristic name="Spell" typeId="1170-c916-1b73-ae34">Drain Magic has a casting value of 9. If successfully cast, all endless spells within 24&quot; of the caster that are not bonded are dispelled.</characteristic>
                  </characteristics>
                </profile>
              </profiles>
              <costs>
                <cost name="pts" typeId="b0b4-6aec-82c2-02ae" value="0.0"/>
