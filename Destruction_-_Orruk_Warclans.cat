                  </constraints>
                  <selectionEntries>
                    <selectionEntry id="01f2-3528-bbf8-2613" name="Bonegrinz" hidden="false" collective="false" import="true" type="upgrade">
                      <profiles>
                        <profile id="a7b0-c3ea-bea9-1db2" name="Barrage of Arrows" hidden="false" typeId="c0c2-177b-e761-ec41" typeName="Allegiance Abilities">
                          <characteristics>
                            <characteristic name="Allegiance Ability" typeId="0506-f62d-68b3-5499">Add 1 to the Attacks characteristic of missile weapons used by friendly BONEGRINZ SAVAGE ORRUK ARROWBOYS.</characteristic>
                          </characteristics>
                        </profile>
                      </profiles>
                      <categoryLinks>
                        <categoryLink id="739f-8009-baa1-5fe9" name="Bonegrinz" hidden="false" targetId="b3e3-9f37-9d39-0a41" primary="false"/>
