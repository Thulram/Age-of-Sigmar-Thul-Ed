                      <profiles>
                        <profile id="5531-ad6f-38a2-e650" name="Strength of Purpose" hidden="false" typeId="c0c2-177b-e761-ec41" typeName="Allegiance Abilities">
                          <characteristics>
                            <characteristic name="Allegiance Ability" typeId="0506-f62d-68b3-5499">Ward rolls cannot be made for wounds and mortal wounds caused by attacks made by friendly DRAKKFOOT units.</characteristic>
                          </characteristics>
