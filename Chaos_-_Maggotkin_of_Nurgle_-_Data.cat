        <profile id="2b48-484e-1783-e93e" name="The Rainfather" hidden="false" typeId="7861-da01-01c0-88d9" typeName="Ability">
          <characteristics>
            <characteristic name="Ability" typeId="9bce-4deb-8b79-eaec">You can re-roll the casting roll when this unit attempts to cast the Deluge of Nurgle spell.</characteristic>
          </characteristics>
        </profile>
        <profile id="f680-c255-7e9d-d3fb" name="Corpulent Mass" hidden="false" typeId="7861-da01-01c0-88d9" typeName="Ability">
          <characteristics>
            <characteristic name="Ability" typeId="9bce-4deb-8b79-eaec">In your hero phase, you can heal D3 wounds that have been allocated to Rotigus.</characteristic>
          </characteristics>
        </profile>
        <profile id="21e8-074e-4b1e-31c7" name="Rotigus" hidden="false" typeId="a62b-2245-5c24-f8bc" typeName="Magic">
