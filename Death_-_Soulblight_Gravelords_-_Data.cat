        <profile id="6e8f-728e-a938-2cbd" name="Vigour of Undeath" hidden="false" typeId="6364-f76d-fc0c-80bc" typeName="Command Abilities">
          <characteristics>
            <characteristic name="Command Ability" typeId="36cb-f8a4-d94b-8aa8">You can use this command ability once per turn in your hero phase. If you do so, add 1 to hit and wound rolls for friendly SOULBLIGHT GRAVELORDS units wholly within 12&quot; of this model until your next hero phase.</characteristic>
          </characteristics>
        </profile>
        <profile id="5040-6b8b-32c8-c10c" name="Mannfred von Carstein" hidden="false" typeId="a62b-2245-5c24-f8bc" typeName="Magic">
