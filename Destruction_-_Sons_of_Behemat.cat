            <selectionEntry id="5614-8653-1ba2-78be" name="4. Strong Right Foot" hidden="false" collective="false" import="true" type="upgrade">
              <profiles>
                <profile id="e015-81bc-779a-b7b7" name="Strong Right Foot" hidden="false" typeId="1504-23bf-8850-4f65" typeName="Command Traits">
