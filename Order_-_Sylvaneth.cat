                      <profiles>
                        <profile id="dfcf-645e-2915-c27a" name="Lacerating Strike" hidden="false" typeId="7861-da01-01c0-88d9" typeName="Ability">
                          <characteristics>
                            <characteristic name="Use" typeId="9bce-4deb-8b79-eaec">Passive</characteristic>
                            <characteristic name="Declare" typeId="f9c7-7dae-69e1-f924">-</characteristic>
                            <characteristic name="Effect" typeId="7d15-5e9e-83a9-d696">Add 1 to the Rend characteristic of melee weapons used by friendly SYLVANETH non-KURNOTHI INFANTRY units that charged in the same turn.</characteristic>
                          </characteristics>
                        </profile>
                      </profiles>
                      <costs>
                        <cost name="pts" typeId="b0b4-6aec-82c2-02ae" value="0.0"/>
                      </costs>
                    </selectionEntry>
                    <selectionEntry id="e112-e3e1-fbab-5a33" name="Outcasts" hidden="false" collective="false" import="true" type="upgrade">
                      <profiles>
                        <profile id="5578-2bc9-7c7a-6832" name="The Terror in the Eaves" hidden="false" typeId="7861-da01-01c0-88d9" typeName="Ability">
                          <characteristics>
                            <characteristic name="Use" typeId="9bce-4deb-8b79-eaec">Passive</characteristic>
                            <characteristic name="Declare" typeId="f9c7-7dae-69e1-f924"/>
                            <characteristic name="Effect" typeId="7d15-5e9e-83a9-d696">Subtract 3 from the control scores of enemy units while they are in combat with friendly SYLVANETH units.</characteristic>
                          </characteristics>
                        </profile>
                      </profiles>
                      <costs>
                        <cost name="pts" typeId="b0b4-6aec-82c2-02ae" value="0.0"/>
                      </costs>
                    </selectionEntry>
                    <selectionEntry id="b8a0-0c20-cfaf-8fb5" name="Free Spirits" hidden="false" collective="false" import="true" type="upgrade">
                      <profiles>
                        <profile id="b98e-2ada-da52-513f" name="Fleet and Swift" hidden="false" typeId="7861-da01-01c0-88d9" typeName="Ability">
                          <characteristics>
                            <characteristic name="Use" typeId="9bce-4deb-8b79-eaec">Once Per Turn (Army), Your Charge Phase</characteristic>
                            <characteristic name="Declare" typeId="f9c7-7dae-69e1-f924">Pick a friendly SYLVANETH CAVALRY unit to use this ability.</characteristic>
                            <characteristic name="Effect" typeId="7d15-5e9e-83a9-d696">This unit can use CHARGE abilities even if it used a RUN ability this turn.</characteristic>
                          </characteristics>
                        </profile>
                      </profiles>
                      <costs>
                        <cost name="pts" typeId="b0b4-6aec-82c2-02ae" value="0.0"/>
                      </costs>
