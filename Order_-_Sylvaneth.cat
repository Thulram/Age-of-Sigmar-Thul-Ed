              <selectionEntries>
                <selectionEntry id="287f-12dc-c8cf-48ea" name="The Burgeoning" hidden="false" collective="false" import="true" type="upgrade">
                  <profiles>
                    <profile id="bfc6-0fc7-3c6d-6883" name="The Burgeoning" hidden="false" typeId="c0c2-177b-e761-ec41" typeName="Allegiance Abilities">
                      <characteristics>
                        <characteristic name="Allegiance Ability" typeId="0506-f62d-68b3-5499">Friendly SYLVANETH units that did not charge in the same turn and are wholly within 9&quot; of an overgrown terrain feature or friendly Awakened Wyldwood have a ward of 6+.</characteristic>
                      </characteristics>
                    </profile>
                  </profiles>
                  <costs>
                    <cost name="pts" typeId="b0b4-6aec-82c2-02ae" value="0.0"/>
                  </costs>
                </selectionEntry>
                <selectionEntry id="c6f5-37b2-2137-453a" name="The Reaping" hidden="false" collective="false" import="true" type="upgrade">
                  <profiles>
                    <profile id="384a-eb86-e029-794c" name="The Reaping" hidden="false" typeId="c0c2-177b-e761-ec41" typeName="Allegiance Abilities">
                      <characteristics>
                        <characteristic name="Allegiance Ability" typeId="0506-f62d-68b3-5499">Add 3&quot; to the range within which you can pick friendly SYLVANETH units with the Places of Power and From the Woodland Depths battle traits.</characteristic>
                      </characteristics>
                    </profile>
