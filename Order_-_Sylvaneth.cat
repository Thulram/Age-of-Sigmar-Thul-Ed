        <selectionEntry id="20f7-b54c-f3f2-7064" name="Sylvaneth" publicationId="41a4-7d44-770a-c56b" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="29f1-8e09-28a1-c1f5" type="min"/>
