              <profiles>
                <profile id="2e9d-5d31-1a53-7426" name="Greenwood Gladius" hidden="false" typeId="0c9d-7abe-a53c-6a37" typeName="Artefacts">
                  <characteristics>
                    <characteristic name="Artefact" typeId="44b6-de26-0b54-1318">Pick 1 of the bearer’s melee weapons. At the start of the combat phase, roll a D3. Add the result to the Attacks characteristic of that weapon until the end of that phase.</characteristic>
                  </characteristics>
                </profile>
              </profiles>
              <costs>
                <cost name="pts" typeId="b0b4-6aec-82c2-02ae" value="0.0"/>
              </costs>
            </selectionEntry>
            <selectionEntry id="904a-b4c1-abf9-f716" name="2 -- Crown of Fell Bowers" hidden="false" collective="false" import="true" type="upgrade">
              <constraints>
                <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" id="e1f8-9dfc-e8a6-a311" type="max"/>
