            <characteristic name="Control" typeId="2420-1c5c-1016-7033">-</characteristic>
          </characteristics>
        </profile>
        <profile id="c8c8-7218-a8e0-3bf1" name="Awakened Wyldwood" hidden="false" typeId="c23b-5a30-07e2-4706" typeName="Warscroll Extras">
          <characteristics>
            <characteristic name="WIZARD" typeId="67b3-6f8b-2116-2a27">-</characteristic>
            <characteristic name="PRIEST" typeId="02ab-4d58-5c31-767e">-</characteristic>
            <characteristic name="WARD" typeId="40a4-4432-32da-74d9">-</characteristic>
            <characteristic name="CHAMPION" typeId="dccd-b70a-d225-5e37">-</characteristic>
            <characteristic name="MUSICIAN" typeId="053d-5038-d84a-3aa3">-</characteristic>
            <characteristic name="STANDARD BEARER" typeId="ccb7-3ee4-5637-586e">-</characteristic>
          </characteristics>
        </profile>
        <profile id="397b-b8da-dfd1-c432" name="Deploy Wyldwood [DEPLOY TERRAIN]" hidden="false" typeId="7861-da01-01c0-88d9" typeName="Ability">
          <characteristics>
            <characteristic name="Use" typeId="9bce-4deb-8b79-eaec">Deployment Phase</characteristic>
            <characteristic name="Declare" typeId="f9c7-7dae-69e1-f924">-</characteristic>
            <characteristic name="Effect" typeId="7d15-5e9e-83a9-d696">Set up this terrain feature wholly within friendly territory, more than 3&quot; from all objectives and other terrain features. This terrain feature has now been deployed.</characteristic>
          </characteristics>
        </profile>
        <profile id="4837-09da-3f94-6c9b" name="Growing Woods" hidden="false" typeId="7861-da01-01c0-88d9" typeName="Ability">
          <characteristics>
            <characteristic name="Use" typeId="9bce-4deb-8b79-eaec">Passive</characteristic>
            <characteristic name="Declare" typeId="f9c7-7dae-69e1-f924">-</characteristic>
            <characteristic name="Effect" typeId="7d15-5e9e-83a9-d696">Each time you set up an Awakened Wyldwood, you can place 1-3 scenery pieces. If you place more than 1 scenery piece, the tips at the end of each base must touch, forming a circle. Add 2 to the Health characteristic of this terrain feature for each additional scenery piece that is placed after the first.</characteristic>
          </characteristics>
        </profile>
        <profile id="bf11-df4f-a10a-0603" name="Overgrown Wilderness" hidden="false" typeId="7861-da01-01c0-88d9" typeName="Ability">
          <characteristics>
            <characteristic name="Use" typeId="9bce-4deb-8b79-eaec">Passive</characteristic>
            <characteristic name="Declare" typeId="f9c7-7dae-69e1-f924"/>
            <characteristic name="Effect" typeId="7d15-5e9e-83a9-d696">Enemy units can be targeted by shooting attacks made by friendly SYLVANETH units even if they are behind or wholly on this terrain feature (this is an exception to Terrain, 1.2).</characteristic>
          </characteristics>
        </profile>
        <profile id="604a-31a5-71c9-a5d6" name="Vengeful Spirits" hidden="false" typeId="7861-da01-01c0-88d9" typeName="Ability">
          <characteristics>
            <characteristic name="Use" typeId="9bce-4deb-8b79-eaec">Once Per Turn (Army), Any Combat Phase</characteristic>
            <characteristic name="Declare" typeId="f9c7-7dae-69e1-f924">Pick each enemy unit within this terrain feature’s combat range to be the targets.</characteristic>
            <characteristic name="Effect" typeId="7d15-5e9e-83a9-d696">Roll a dice for each target. On a 4+, inflict D3 mortal damage on the target.</characteristic>
          </characteristics>
        </profile>
      </profiles>
      <rules>
        <rule id="5ab8-c5c9-1478-1f8f" name="Universal Terrain Abilities" hidden="false">
          <description>The following universal terrain abilities apply to this terrain feature:

Cover, Unstable, Obscuring</description>
        </rule>
      </rules>
