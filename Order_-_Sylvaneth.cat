          <selectionEntries>
            <selectionEntry id="75b7-d704-2cf0-3db5" name="1 -- Greenwood Gladius" hidden="false" collective="false" import="true" type="upgrade">
              <constraints>
                <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" id="b014-0120-e3c0-b54c" type="max"/>
