      <profiles>
        <profile id="c45d-8b6e-6b7e-93d7" name="Tyrant" hidden="false" typeId="3956-f6dd-6c88-ae4b" typeName="Unit">
          <characteristics>
            <characteristic name="Move" typeId="5bbe-8910-4dfa-7b1c">6&quot;</characteristic>
            <characteristic name="Wounds" typeId="a0c6-af64-2f24-5759">8</characteristic>
