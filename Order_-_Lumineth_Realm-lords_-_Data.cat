            <characteristic name="Use" typeId="9bce-4deb-8b79-eaec">Passive</characteristic>
            <characteristic name="Declare" typeId="f9c7-7dae-69e1-f924"/>
            <characteristic name="Effect" typeId="7d15-5e9e-83a9-d696">Add 1 to casting rolls for other friendly LUMINETH REALM-LORDS WIZARDS while they are wholly within 12&quot; of this unit.</characteristic>
          </characteristics>
        </profile>
