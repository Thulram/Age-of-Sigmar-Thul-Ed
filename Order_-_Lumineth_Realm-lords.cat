      <selectionEntries>
        <selectionEntry id="4ec4-58f7-ff5c-8b2f" name="Lumineth Realm-lords" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="e431-e3bb-00ba-27d2" type="min"/>
