    <selectionEntry id="642e-4e87-03bd-b99b" name="Auralan Legion" publicationId="fe71-80f7-f53f-3348" hidden="false" collective="false" import="true" type="upgrade">
      <profiles>
        <profile id="458a-935e-67b5-4ac7" name="Shield of Light" hidden="false" typeId="66c5-932e-004a-574c" typeName="Battalion Abilities">
          <characteristics>
            <characteristic name="Battalion Ability" typeId="23f7-32c0-e059-29b2">You can re-roll save rolls of 1 for attacks that target a friendly unit from this battalion while it is within 3&quot; of any other friendly units from the same battalion.</characteristic>
          </characteristics>
        </profile>
      </profiles>
      <categoryLinks>
