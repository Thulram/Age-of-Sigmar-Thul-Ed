            <selectionEntry id="db91-74df-2f29-8351" name="5 - Total Eclipse" hidden="false" collective="false" import="true" type="upgrade">
              <profiles>
                <profile id="7dde-492b-e1c4-6e45" name="Total Eclipse" hidden="false" typeId="13da-11a8-b7d1-77a7" typeName="Spell">
                  <characteristics>
                    <characteristic name="Spell" typeId="1170-c916-1b73-ae34">Total Eclipse has a casting value of 8. If successfully cast, until your next hero phase, your opponent must spend 2 command points to use a command ability instead of 1.</characteristic>
                  </characteristics>
                </profile>
