            <selectionEntry id="97ed-0c2c-dc05-afc8" name="3 - Lambent Light" hidden="false" collective="false" import="true" type="upgrade">
              <profiles>
                <profile id="f8df-b50e-7a0e-a2b8" name="Lambent Light" hidden="false" typeId="13da-11a8-b7d1-77a7" typeName="Spell">
                  <characteristics>
                    <characteristic name="Spell" typeId="1170-c916-1b73-ae34">Lambent Light has a casting value of 5. If successfully cast, pick 1 enemy unit within 18&quot; of the caster and visible to them. Until your next hero phase, you can re-roll hit rolls for attacks made with missile weapons that target that unit.</characteristic>
                  </characteristics>
                </profile>
              </profiles>
              <costs>
                <cost name="pts" typeId="b0b4-6aec-82c2-02ae" value="0.0"/>
