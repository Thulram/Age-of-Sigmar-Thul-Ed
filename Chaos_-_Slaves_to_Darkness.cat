              </constraints>
              <profiles>
                <profile id="4b1a-ec3f-3ce6-2140" name="Diabolic Mantle" hidden="false" typeId="0c9d-7abe-a53c-6a37" typeName="Artefacts">
                  <characteristics>
                    <characteristic name="Artefact" typeId="44b6-de26-0b54-1318">At the start of the first battle round, if the bearer is on the battlefield, you receive D3 command points.</characteristic>
                  </characteristics>
                </profile>
              </profiles>
              <costs>
                <cost name="pts" typeId="b0b4-6aec-82c2-02ae" value="0.0"/>
