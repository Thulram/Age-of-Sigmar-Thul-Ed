              </costs>
            </selectionEntry>
            <selectionEntry id="055b-a0a7-09cc-2f4f" name="2 -- Helm of Many Eyes" hidden="false" collective="false" import="true" type="upgrade">
              <constraints>
                <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" id="8128-5427-a7d1-bf9c" type="max"/>
