      <profiles>
        <profile id="43e0-c88c-a127-d929" name="Chaos Warshrine" hidden="false" typeId="3956-f6dd-6c88-ae4b" typeName="Unit">
          <characteristics>
            <characteristic name="Move" typeId="5bbe-8910-4dfa-7b1c">*</characteristic>
            <characteristic name="Wounds" typeId="a0c6-af64-2f24-5759">12</characteristic>
