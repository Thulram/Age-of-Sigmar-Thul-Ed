      <profiles>
        <profile id="3aa1-72c2-e527-ee9f" name="Warp-Grinder" hidden="false" typeId="3956-f6dd-6c88-ae4b" typeName="Unit">
          <characteristics>
            <characteristic name="Move" typeId="5bbe-8910-4dfa-7b1c">6&quot;</characteristic>
            <characteristic name="Wounds" typeId="a0c6-af64-2f24-5759">3</characteristic>
