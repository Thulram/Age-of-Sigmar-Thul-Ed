        <profile id="0151-58c5-91f3-59e3" name="Wurrgog Prophet" hidden="false" typeId="3956-f6dd-6c88-ae4b" typeName="Unit">
          <characteristics>
            <characteristic name="Move" typeId="5bbe-8910-4dfa-7b1c">5&quot;</characteristic>
            <characteristic name="Wounds" typeId="a0c6-af64-2f24-5759">7</characteristic>
