      <profiles>
        <profile id="2b00-0fba-56be-5f37" name="Commanding Presence" hidden="false" typeId="7861-da01-01c0-88d9" typeName="Ability">
          <characteristics>
            <characteristic name="Ability" typeId="9bce-4deb-8b79-eaec">Subtract 1 from hit rolls for attacks that target this unit.</characteristic>
