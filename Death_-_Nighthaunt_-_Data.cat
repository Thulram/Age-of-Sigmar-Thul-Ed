    <selectionEntry id="18fc-073a-cdfd-ad1f" name="Lady Olynder, Mortarch of Grief" publicationId="6918-4599-9a4b-12c8" hidden="false" collective="false" import="true" type="unit">
      <constraints>
        <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" id="3aed-8e27-ef12-37f5" type="max"/>
