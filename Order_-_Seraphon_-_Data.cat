      <profiles>
        <profile id="0c41-0b84-f9a6-899a" name="Slann Starmaster" hidden="false" typeId="3956-f6dd-6c88-ae4b" typeName="Unit">
          <characteristics>
            <characteristic name="Move" typeId="5bbe-8910-4dfa-7b1c">5&quot;</characteristic>
            <characteristic name="Wounds" typeId="a0c6-af64-2f24-5759">9</characteristic>
