            <selectionEntry id="69ca-6f49-7df0-3a65" name="5 - Phantasmagoria" hidden="false" collective="false" import="true" type="upgrade">
              <constraints>
                <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="6778-78d6-605e-1cf2" type="max"/>
