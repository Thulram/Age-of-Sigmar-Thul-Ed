    <selectionEntry id="571a-8311-be05-4b72" name="Terrain Feature - Skull Altar" hidden="false" collective="false" import="true" type="unit">
      <constraints>
        <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" id="ff38-05ed-ed6c-bfcd" type="max"/>
