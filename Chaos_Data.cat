        <profile id="6b54-1bbf-88e4-c0bd" name="(03) PLAGUEBEARER HOST with 10 models" hidden="false" typeId="a802-7b4b-c415-b5a2" typeName="Contagion Summoning Table">
          <characteristics>
            <characteristic name="Cost" typeId="824b-6b08-47ff-80c3">14</characteristic>
          </characteristics>
